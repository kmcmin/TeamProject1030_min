CREATE TABLE TB_REVIEW(
    REVIEW_NO NUMBER, --게시글 번호
    USER_NO NUMBER,
    USER_ID VARCHAR2(100), --유저 이름
    PRODUCT_CODE VARCHAR2(30), --상품코드
    REVIEW_TITLE VARCHAR2(100),  --리뷰 제목
    REVIEW_CONTENT VARCHAR2(300),  --글 내용
    REVIEW_REGISTRATION DATE DEFAULT SYSDATE,  --등록일
    EVAL_GRADE NUMBER, --별점
    SELLER_COMMENT VARCHAR2(500) --판매자 댓글
);
create SEQUENCE TB_REVIEW_seq;